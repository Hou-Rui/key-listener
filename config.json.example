[
  {
    "name": "Subway Surfers",
    "shell": "adb shell",
    "bindings": [
      {
        "key": "KEY_LEFT",
        "event": "pressed",
        "desc": "Swipe left from center",
        "cmd": "input swipe 960 540 800 540 100 &",
        "useShell": true
      },
      {
        "key": "KEY_RIGHT",
        "event": "pressed",
        "desc": "Swipe right from center",
        "cmd": "input swipe 800 540 960 540 100 &",
        "useShell": true
      },
      {
        "key": "KEY_UP",
        "event": "pressed",
        "desc": "Swipe up from center",
        "cmd": "input swipe 960 540 960 400 100 &",
        "useShell": true
      },
      {
        "key": "KEY_DOWN",
        "event": "pressed",
        "desc": "Swipe down from center",
        "cmd": "input swipe 960 400 960 540 100 &",
        "useShell": true
      },
      {
        "key": "KEY_SPACE",
        "event": "pressed",
        "desc": "Double tap at center",
        "cmd": "input tap 960 540 & sleep 0.1; input tap 960 540 &",
        "useShell": true
      }
    ]
  },
  {
    "name": "Alto's Odyssey",
    "shell": "adb shell",
    "bindings": [
      {
        "key": "KEY_SPACE",
        "event": "pressed",
        "desc": "Press on center",
        "cmd": "input motionevent DOWN 960 540 &",
        "useShell": true
      },
      {
        "key": "KEY_SPACE",
        "event": "released",
        "desc": "Release from center",
        "cmd": "input motionevent UP 960 540 &",
        "useShell": true
      },
      {
        "key": "KEY_ENTER",
        "event": "pressed",
        "desc": "Activate wingsuit",
        "cmd": "input tap 50 540 &",
        "useShell": true
      }
    ]
  },
  {
    "name": "Test Preset",
    "shell": "/bin/sh",
    "bindings": [
      {
        "key": "KEY_LEFT",
        "event": "pressed",
        "desc": "Test left",
        "cmd": "kdialog --msgbox 'left' &",
        "useShell": true
      },
      {
        "key": "KEY_RIGHT",
        "event": "pressed",
        "desc": "Test right",
        "cmd": "kdialog --msgbox 'right' &",
        "useShell": true
      }
    ]
  }
]